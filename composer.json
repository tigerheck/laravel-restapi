{
    "name": "tigerheck/laravel-restapi",
    "type": "library",
    "license": "MIT",
    "description": "Laravel wrapper for RestApi APIs",
    "keywords": [
        "laravel",
        "restapi"
    ],
    "authors": [
        {
            "name": "Chirag Goti",
            "email": "gotichirag2011@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "TigerHeck\\RestApi\\RestApiServiceProvider"
            ],
            "aliases": {
                "RestApi": "TigerHeck\\RestApi\\RestApiFacade"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "TigerHeck\\RestApi\\": "src"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}